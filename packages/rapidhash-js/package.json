{
  "name": "rapidhash-js",
  "version": "1.0.3",
  "description": "A TypeScript implementation of Rapidhash",
  "keywords": [
    "hash",
    "rapidhash"
  ],
  "author": {
    "name": "KOMIYA Atsushi",
    "url": "https://github.com/komiya-atsushi"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/komiya-atsushi/rapidhash-js.git"
  },
  "bugs": {
    "url": "https://github.com/komiya-atsushi/rapidhash-js/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
